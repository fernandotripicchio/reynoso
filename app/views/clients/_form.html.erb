
<fieldset>	
	<%= f.input :name, :placeholder => "nombre", :label => "Nombre:", :input_html => {class: 'input-xxlarge'   }  %>
	<%= f.input :address, :placeholder => "direccion" ,:label => "Dirección:",  :input_html => { class: 'input-xxlarge' }  %>
	<%= f.input :email, :placeholder => "email",:label => "Email:",  :input_html => {  class: 'input-medium' }  %>
	<%= f.input :phone, :placeholder => "telefono",:label => "Teléfono:",  :input_html => {  class: 'input-medium' }  %>
    <%= f.input :province, :collection => provincies, :label => "Provincia:" , :prompt => "Seleccione una provincia" %>
    <%= f.input :zip_code, :placeholder => "codigo postal",:label => "CP:",  :input_html => {  class: 'input-medium' }  %>    	
	<%= f.input :comments, :as => :text, :placeholder => "Observaciones",:label => "Observaciones:",  :input_html => { class: 'input-xxlarge'  }  %>    
</fieldset>   