<div id="edit_form" class="form_project">  	
	<legend>Sucursal <%= @branch.name %> - Editar Producto </legend>
  	<%= simple_form_for @product , :url => branch_product_path(@branch, @product),:html => { :class => "form-horizontal" } do |f| %>
  	<%= render :partial => "form", :locals => {:f => f}%>
  	<hr />
    <%= link_to "Cancelar", branch_products_path(@branch), :class => "btn btn-secundary btn-form"%>
    <%= f.submit "Actualizar", :class => "btn btn-primary btn-form"%>
    <% end %>
</div>
