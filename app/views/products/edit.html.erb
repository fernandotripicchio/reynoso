<div id="edit_form" class="form_project">  	
	<legend>Editar Producto </legend>
  	<%= simple_form_for @product ,  :html => { :class => "form-horizontal" } do |f| %>
  	<%= render :partial => "form", :locals => {:f => f}%>
  	<hr />
    <%= link_to "Cancelar", products_path, :class => "btn btn-secundary btn-form"%>
    <%= f.submit "Actualizar", :class => "btn btn-primary btn-form"%>
    <% end %>
</div>
