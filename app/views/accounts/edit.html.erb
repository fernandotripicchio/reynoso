<div id="edit_form" class="form_project">  	
	<legend>Cuenta Corriente </legend>
  	<%= simple_form_for @account ,:url => branch_account_path(@branch) , :html => { :class => "form-horizontal" } do |f| %>
  	<%= render :partial => "form_edit", :locals => {:f => f}%>
  	<hr />
    <%= link_to "Cancelar", branch_accounts_path(@branch), :class => "btn btn-secundary btn-form"%>
    <%= f.submit "Actualizar", :class => "btn btn-primary btn-form", :id => "actualizarSaldo"%>
    <% end %>
</div>
